  "scripts": {
    "prepare": "cd ../ && husky install",
    "build": "npm-run-all --parallel \"build:brizy -- {@}\" \"server\" -- ",
    "build:brizy": "gulp build",
    "build:server": "NODE_ENV=production webpack build --config ./webpack.config.server.js",
    "build:module": "rimraf build && webpack build --config ./webpack.config.module.js",
    "build:github": "npm run build:brizy -- -t WP --build-dir ../editor-build",
    "build:opensource": "gulp opensource",
    "external_popups": "gulp external_popups",
    "server": "node ./backend/server.js",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./editor/js",
    "check": "npm run tsc && npm run test && npm run lint",
    "analyze:export": "gulp analyze_export --analyze_export -t Cloud -e -p --production --version=analyze_export --version-pro=analyze_export-pro",
    "analyze:preview": "gulp analyze_preview --analyze_preview -t Cloud -e -p --production --version=analyze_preview --version-pro=analyze_preview-pro"
  },
