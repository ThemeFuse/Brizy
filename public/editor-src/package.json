  "scripts": {
    "prepare": "cd ../ && husky install",
    "build": "npm-run-all --parallel \"build:brizy -- {@}\" \"server -- {@}\" \"compiler\" --",
    "build:brizy": "gulp build",
    "build:server": "NODE_ENV=production webpack build --config ./webpack.config.server.js",
    "external_popups": "gulp external_popups",
    "server": "node ./backend/server.js",
    "compiler": "NODE_ENV=development PORT=5000 npm start --prefix=../../editor.compiler",
    "build:github": "npm run build:brizy -- -t WP --build-dir ../editor-build",
    "test": "jest",
    "tsc": "tsc",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./editor/js",
    "check": "npm run tsc && npm run test && npm run lint",
    "cms:graphql:schema:update": "apollo service:download --endpoint=${ENDPOINT} editor/js/utils/api/cms/graphql/schema.json && graphql-introspection-json-to-sdl editor/js/utils/api/cms/graphql/schema.json > editor/js/utils/api/cms/graphql/schema.graphql",
    "cms:graphql:codegen": "rm -rf editor/js/utils/api/cms/graphql/types/* && apollo client:codegen --localSchemaFile=editor/js/utils/api/cms/graphql/schema.graphql --target=typescript --includes=editor/js/utils/api/**/*.ts --outputFlat --globalTypesFile=editor/js/utils/api/cms/graphql/types/entities.ts editor/js/utils/api/cms/graphql/types",
    "analyze:export": "gulp analyze_export --analyze_export -t Cloud -e -p --production --version=analyze_export --version-pro=analyze_export-pro",
    "analyze:preview": "gulp analyze_preview --analyze_preview -t Cloud -e -p --production --version=analyze_preview --version-pro=analyze_preview-pro"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
