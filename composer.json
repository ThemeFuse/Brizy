{
  "name": "brizy/brizy",
  "description": "A free drag & drop framework that comes with a bunch of built in extensions that will help you develop premium themes fast & easy.",
  "minimum-stability": "dev",
  "license": "GPL2+",
  "authors": [
    {
      "name": "Brizy.io"
    }
  ],
  "require": {
    "php": ">=5.6",
    "twig/twig": "1.42.5",
    "knplabs/gaufrette": "0.7",
    "bagrinsergiu/brizy-migration-utils": "^1.4",
    "bagrinsergiu/brizy-merge-page-assets": "dev-master",
    "bagrinsergiu/content-placeholder": "2.0.6",
    "enshrined/svg-sanitize": "^0.13",
    "select2/select2": "^4.0",
    "shortpixel/shortpixel-php": "dev-master",
    "symfony/dotenv": "^3",
    "tburry/pquery": "dev-master"
  },
  "config": {
    "platform-check": false,
    "platform": {
      "php": "5.6"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:bagrinsergiu/brizy-migration-utils.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:bagrinsergiu/brizy-merge-page-assets.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/bagrinsergiu/brizy-content-placeholder.git"
    }
  ],
  "scripts": {
    "phpcs": "phpcs --standard=phpcs.xml"
  }
}
